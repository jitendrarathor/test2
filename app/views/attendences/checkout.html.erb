<%= form_for @attendence, :url => attendences_path do |f| %>
	<table>
		 <tr>
		   <th>OutDate</th>
		   <th>OutTime</th>
		   <th>WorkHour</th>
		 </tr>
		  <td><%= f.text_field :out_date %></td>
		  <td><%= f.text_field :out_time %></td>
		  <td><%= f.text_field :workhour %></td>
		 </tr>
	</table>
 <%= f.submit "Update checkin" %>
 <% end %>

